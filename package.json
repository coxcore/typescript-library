{
  "name": "cox-typescript-library",
  "version": "0.0.0",
  "description": "typescript library boilerplate",
  "main": "build/index.min.js",
  "unpkg": "build/index.min.js",
  "jsdelivr": "build/index.min.js",
  "module": "build/index.js",
  "umd": "build/index.min.js",
  "types": "build/types/index.d.ts",
  "scripts": {
    "install": "node ./scripts/initialize",
    "start": "webpack serve",
    "build": "rollup -c && tsc",
    "build:demo": "webpack",
    "test": "jest --silent",
    "coverage": "npm run test -- --coverage",
    "prepare": "npm run build",
    "trypublish": "npm publish || true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coxcore/typescript-library"
  },
  "author": "Park U-yeong <ascript0@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coxcore/typescript-library/issues"
  },
  "homepage": "https://github.com/coxcore/typescript-library",
  "keywords": [
    "library",
    "javascript",
    "typescript",
    "js",
    "ts"
  ],
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-typescript": "^7.15.4",
    "@babel/preset-env": "^7.15.6",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/estree": "0.0.47",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "acorn": "^8.5.0",
    "babel-loader": "^8.2.2",
    "babel-preset-minify": "^0.5.1",
    "css-loader": "^6.3.0",
    "eslint": "^7.32.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.2.2",
    "mini-css-extract-plugin": "^2.3.0",
    "prettier": "^2.4.1",
    "rollup": "^2.57.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "style-loader": "^3.3.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.54.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/lib/index.js",
      "!src/demo/index.js",
      "!src/utils/**",
      "!**/tests/**"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/scripts/testMock.js",
      "\\.(css|less)$": "<rootDir>/scripts/testMock.js"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
